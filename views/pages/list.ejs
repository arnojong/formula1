<div id="list" v-cloak>

  <div class="container">
    <% posts.forEach(function(post){ %>
    <h3><%= post.title %></h3>
    <h6>by <%= post.createdBy %></h6>

    <p><%= post.body %></p>
    <br>
    <% }) %>
  </div>

</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
